public MicrowaveTileEntity(Container p_i1072_1_) {
	super();
}
IInventory playerInv, ModTileEntity;
private int xSize;
private int ySize;
private Object guiLeft;
private Object guiTop;
private Object fontRendererObj;
private Object mc;
private Object te; te) {
    super(new MicrowaveTileEntity(playerInv, te));

    this.playerInv = playerInv;
    this.te = te;

