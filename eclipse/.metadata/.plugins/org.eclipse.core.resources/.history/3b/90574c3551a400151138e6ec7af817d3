    this.ySize = 166;
}

protected void drawGuiContainerForegroundLayer(int mouseX, int mouseY) {
}


protected void drawGuiContainerBackgroundLayer(float partialTicks, int mouseX, int mouseY) {
    GlStateManager.color(1.0f, 1.0f, 1.0f, 1.0f);
    ((Object) this.mc).getTextureManager().bindTexture(new ResourceLocation("/gui/container/mod_tile_entity.png"));
    this.drawTexturedModalRect(this.guiLeft, this.guiTop, 0, 0, this.xSize, this.ySize);
{
    String s = ((Object) this.te).getDisplayName().getUnformattedText();
    this.fontRendererObj.drawString(s, 88 - ((Object) this.fontRendererObj).getStringWidth(s) / 2, 6, 4210752);            //#404040
    this.fontRendererObj.drawString(((Object) this.playerInv).getDisplayName().getUnformattedText(), 8, 72, 4210752);      //#404040
}
}


