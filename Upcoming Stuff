   //Blocks
  public static Block

  //Banana
    itembanana = new ItemFood(3, 0.2F, true).setUnlocalizedName("itembanana").setTextureName("foodmod:itembanana").setCreativeTab(FoodMod);
    GameRegistry.registerItem(itembanana, itembanana.getUnlocalizedName().substring(5));
    
